<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <RootNamespace>Clash.SDK</RootNamespace>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
    <PackageId>CoelWu.Clash.SDK</PackageId>
    <Authors>Coel Wu</Authors>
    <Product>Clash.SDK</Product>
    <PackageLicenseExpression>Apache-2.0</PackageLicenseExpression>
    <Description>Clash Proxy Controller via C#</Description>
    <PackageTags>clash, proxy, sdk</PackageTags>
    <Copyright>Copyright (c) 2018-2021 Coel Wu.</Copyright>
    <PackageProjectUrl>https://github.com/CoelWu/Clash-SDK</PackageProjectUrl>
    <RepositoryUrl>https://github.com/CoelWu/Clash-SDK</RepositoryUrl>
    <PackageIcon>Clash_icon.png</PackageIcon>
    <Version>1.1.0</Version>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.CSharp" Version="4.7.0" />
    <PackageReference Include="Newtonsoft.Json" Version="12.0.3" />
    <PackageReference Include="WebSocketSharp-netstandard" Version="1.0.1" />
  </ItemGroup>

  <ItemGroup>
    <None Include="Assets\Clash_icon.png">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

</Project>
